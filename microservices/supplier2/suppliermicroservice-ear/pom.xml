<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>suppliermicroservice-ear</groupId>
  <artifactId>suppliermicroservice-ear</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>ear</packaging>
  <dependencies>
  
   <dependency>
      <groupId>suppliermicroservice-jpa</groupId>
      <artifactId>suppliermicroservice-jpa</artifactId>
      <version>0.0.1-SNAPSHOT</version>
    </dependency>
    
    <dependency>
      <groupId>suppliermicroservice-ejb</groupId>
      <artifactId>suppliermicroservice-ejb</artifactId>
      <version>0.0.1-SNAPSHOT</version>
      <type>ejb</type>
    </dependency>
    <!-- https://mvnrepository.com/artifact/org.jboss/jboss-common-core -->
<dependency>
    <groupId>org.jboss</groupId>
    <artifactId>jboss-common-core</artifactId>
    <version>2.5.0.Final</version>
</dependency>

<!-- https://mvnrepository.com/artifact/org.jboss.as/jboss-as-deployment-repository -->
<dependency>
    <groupId>org.jboss.as</groupId>
    <artifactId>jboss-as-deployment-repository</artifactId>
    <version>7.1.3.Final</version>
</dependency>

    
    </dependencies>
    
    
    
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-ear-plugin</artifactId>
        <version>3.0.2</version>
        
        <configuration>
          <earSourceDirectory>EarContent</earSourceDirectory>
          <generateApplicationXml>false</generateApplicationXml>
          <version>8</version>
          <defaultLibBundleDir>lib</defaultLibBundleDir>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>